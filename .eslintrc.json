{
  "extends": ["airbnb", "prettier/react"],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    //React Specific
    //allow jsx synatx to live in js files
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "prettier/prettier": "error"
  }
}
